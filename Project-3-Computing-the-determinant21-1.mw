<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">#copied from lab 3 solutions
ourdet:=proc(A)
   local m,n,det,j;
   m,n := op(1,A); # get the dimensions of the matrix (can be done other ways)
   if m&lt;&gt;n then
      return &quot;Error: matrix must be square&quot;;
   elif n=1 then
      return A[1,1];
   else
      det:=0:
      for j from 1 to n do
         det:=det+(-1)^(1+j)*A[1,j]*ourdet( A[2..-1,[1..j-1,j+1..-1]] );
      end do:
      return det;
   end if:
end proc: </Text-field>
</Input>

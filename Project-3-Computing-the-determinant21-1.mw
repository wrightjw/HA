<Group view="code" labelreference="L1269" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">sgn := proc(mypermutation, N)
   local i, j, sum;
   sum := 0;
   for i from 1 to N-1 do
      for j from i+1 to N do
         if mypermutation[i] &gt; mypermutation[j] then
            sum := sum + 1;
         end if:
      end do:
   end do:
   return (-1)^sum;
end proc:</Text-field>
</Input>

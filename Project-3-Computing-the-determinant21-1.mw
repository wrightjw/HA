<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">makeLU := proc(A)
   local U, L, P, M, N, i, j, k, mypermutation, ones, entry:
   M := ColumnDimension(A): 
   N := RowDimension(A):
   ones := [seq(1,i=1..N)];
   U := copy(A):
   L := DiagonalMatrix(ones):
   P := DiagonalMatrix(ones):
   mypermutation := [seq(i,i=1..N)];
   
   if M &lt;&gt; N then
      error(&quot;Not NxN Matrix!&quot;);
   else
      for k from 1 to N-1 do
         #find i greater than or equal to k such that magnitude of i-th entry in k-th column is greatest
         i := k-1+max[index](abs(U[k..-1,k]));
         swaprow(P, i,k,1,-1);     
         swaprow(U, i,k,k,N);
         swaprow(L, i,k,1,k-1);
         #the i-th entry of mypermutation identifies where the i-th row of the identity is mapped
         entry := mypermutation[i];
         mypermutation[i]:= mypermutation[k];
         mypermutation[k] := entry;
         for j from k+1 to N do
            if U[k,k] = 0 then 
               error(&quot;Zeros on and below a diagonal on column - undecomposible.&quot;)
            end if:
            L[j,k] := U[j,k]/U[k,k];
            U[j,k..N] := U[j,k..N]-L[j,k]*U[k,k..N];
         end do:
      end do:
      #returning transpose of P
      return P^%T,L,U,sgn(mypermutation, N);
   end if:
end proc:</Text-field>
</Input>
</Group></Presentation-Block>

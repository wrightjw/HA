<Group view="code" hide-input="false" labelreference="L1354" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">test_PLU := proc(size,n)
   local i, X, LU, A, passtest;
   
   passtest := true;
   
   for i from 1 to n do:
      X := RandomMatrix(size);
      LU := makeLU(X);
      A := LU[1].LU[2].LU[3];
      passtest := passtest and Equal(A,X);
   end do:
   return passtest;
end proc:

test_PLU(5,100);</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L1366" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">test_det := proc(size,n)
   local i, X, mydet, mapledet, passtest;
   
   passtest := true;
   
   for i from 1 to n do:
      X := RandomMatrix(size);
      mydet := fastDet(X);
      mapledet := Determinant(X);
      passtest := passtest and evalb(mydet=mapledet);
   end do:
   return passtest;
end proc:

test_det(5,100);</Text-field>
</Input>
</Group></Presentation-Block>
